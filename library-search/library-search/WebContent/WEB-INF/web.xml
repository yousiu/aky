<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>library-search</display-name>
  <context-param>
    <param-name>/login.do</param-name>
    <param-value>controller.LoginAction</param-value>
  </context-param>
  <context-param>
    <param-name>/loginback.do</param-name>
    <param-value>controller.LoginBackAction</param-value>
  </context-param>
   <context-param>
    <param-name>/book-registration.do</param-name>
    <param-value>controller.BookRegistrationAction</param-value>
  </context-param>
   <context-param>
   <param-name>/bookRegistration.do</param-name>
    <param-value>controller.BookRegistration</param-value>
  </context-param>
  <context-param>
    <param-name>/mapback.do</param-name>
    <param-value>controller.MapBackAction</param-value>
  </context-param>
  <context-param>
    <param-name>/sign-up.do</param-name>
    <param-value>controller.SignUpAction</param-value>
  </context-param>
  <servlet>
    <servlet-name>login</servlet-name>
    <jsp-file>/login.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>top</servlet-name>
    <jsp-file>/top.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>top</servlet-name>
    <url-pattern>/top</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>sign-up</servlet-name>
    <jsp-file>/sign-up.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>sign-up</servlet-name>
    <url-pattern>/sign-up</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>library-address</servlet-name>
    <jsp-file>/library-address.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>library-address</servlet-name>
    <url-pattern>/library-address</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>map-after</servlet-name>
    <jsp-file>/map-after.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>map-after</servlet-name>
    <url-pattern>/map-after</url-pattern>
  </servlet-mapping>
 <servlet>
    <servlet-name>book-registration</servlet-name>
    <jsp-file>/book-registration.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>book-registration</servlet-name>
    <url-pattern>/book-registration</url-pattern>
  </servlet-mapping>
</web-app>